    "layer": [
        {
            "description": "The normal range band around the line",
            "mark": {
                "borders": {
                    "color": {
                        "expr": "lineColor"
                    },
                    "opacity": {
                        "expr": "normalRangeOpacity"
                    },
                    "strokeWidth": {
                        "expr": "normalRangeStrokeWidth"
                    }
                },
                "type": "errorband",
                "color": {
                    "expr": "normalRangeColor"
                },
                "opacity": {
                    "expr": "normalRangeOpacity"
                }
            },
            "encoding": {
                "y": {
                    "type": "quantitative",
                    "scale": {
                        "zero": false
                    },
                    "title": "Normal range",
                    "axis": null,
                    "field": "ci1"
                },
                "y2": {
                    "field": "ci0"
                }
            },
            "name": "normal-range"
        },
        {
            "mark": {
                "type": "line",
                "clip": true,
                "stroke": {
                    "expr": "\n            if(datum.changeSentiment === 'positive', colorDesiredChange,\n              if(datum.changeSentiment === 'negative', colorUndesiredChange, lineColor)\n            )\n          "
                },
                "strokeWidth": {
                    "expr": "lineStrokeWidth"
                },
                "opacity": 1
            },
            "encoding": {
                "color": {
                    "type": "ordinal",
                    "legend": null,
                    "field": "changeSentiment"
                },
                "strokeDash": {
                    "legend": null,
                    "condition": {
                        "test": "datum.dashed",
                        "value": {
                            "expr": "lineStrokeDash"
                        }
                    },
                    "field": "dashed",
                    "type": "nominal"
                },
                "y": {
                    "field": "rawValue",
                    "type": "quantitative",
                    "axis": null
                },
                "detail": {
                    "field": "segment",
                    "type": "nominal"
                }
            },
            "name": "line",
            "description": "The line based of time series data"
        },
        {
            "description": "Manual left x-axis grid line to overlay the errorband that occludes the real axis grid line",
            "mark": {
                "type": "rule",
                "color": {
                    "expr": "datum.truncDate === activeLabelAndGridDate ? xAxisGridColorActive : xAxisGridColorDefault"
                },
                "opacity": {
                    "expr": "datum.truncDate === activeLabelAndGridDate || (length(data('series')) > 0 && datum.truncDate === peek(data('series')).truncDate ? 1 : 0)"
                }
            },
            "name": "left-x-axis-overlay"
        },
        {
            "mark": {
                "opacity": 1,
                "strokeWidth": {
                    "expr": "currentValueCircleStrokeWidth"
                },
                "type": "point",
                "filled": true,
                "fill": {
                    "expr": "currentValueCircleColor1"
                },
                "size": {
                    "expr": "datum.showIsolatedPoint*currentValueCircleSize"
                }
            },
            "encoding": {
                "y": {
                    "type": "quantitative",
                    "axis": null,
                    "field": "rawValue"
                }
            },
            "name": "isolated-point-value-circle",
            "description": "Circle for isolated points in data series"
        },
        {
            "encoding": {
                "y": {
                    "axis": {
                        "offset": {
                            "expr": "leftYAxisOffset"
                        },
                        "values": {
                            "expr": "yAxisLabelsDataValues"
                        },
                        "grid": false,
                        "format": {
                            "custom": true,
                            "mapName": "yAxis"
                        },
                        "labelAlign": "right",
                        "labelBaseline": {
                            "condition": {
                                "test": "datum.value === 0",
                                "value": "bottom"
                            },
                            "value": "middle"
                        }
                    },
                    "field": "rawValue",
                    "type": "quantitative"
                }
            },
            "name": "current-value-circle",
            "description": "Circle at the end of the line on the current data point",
            "mark": {
                "filled": true,
                "fill": {
                    "expr": "\n            if(lastPointCircleMode === 'empty', currentValueCircleColor2,\n              if(datum.changeSentiment === 'positive', colorDesiredChange,\n                if(datum.changeSentiment === 'negative', colorUndesiredChange, currentValueCircleColor1)\n              )\n            )\n          "
                },
                "size": {
                    "expr": "datum.point*currentValueCircleSize"
                },
                "opacity": 1,
                "stroke": {
                    "expr": "lastPointCircleMode === 'empty' ? currentValueCircleColor1 : currentValueCircleColor2 "
                },
                "strokeWidth": {
                    "expr": "currentValueCircleStrokeWidth"
                },
                "type": "point"
            }
        },
        {
            "name": "current-value-text",
            "description": "The current value text after the end of the line",
            "mark": {
                "font": {
                    "expr": "currentValueFontFace"
                },
                "fontWeight": {
                    "expr": "currentValueFontWeight"
                },
                "align": "left",
                "text": {
                    "expr": "datum.point === 1 ? datum.formattedRawValue : \"\""
                },
                "dx": {
                    "expr": "currentValuePadding"
                },
                "type": "text",
                "color": {
                    "expr": "\n            if(lastPointCircleMode === 'empty', currentValueColor,\n              if(datum.changeSentiment === 'positive', colorDesiredChange,\n                if(datum.changeSentiment === 'negative', colorUndesiredChange, currentValueColor)\n              )\n            )\n          "
                },
                "fontSize": {
                    "expr": "currentValueFontSize"
                }
            },
            "encoding": {
                "y": {
                    "field": "rawValue",
                    "type": "quantitative",
                    "axis": null
                }
            }
        },
        {
            "description": "Hover for tooltips containing a single time series data point",
            "mark": {
                "type": "rule",
                "color": {
                    "expr": "hoverVerticalLineColor"
                }
            },
            "encoding": {
                "opacity": {
                    "condition": {
                        "value": {
                            "expr": "hoverVerticalLineOpacity"
                        },
                        "param": "hover",
                        "empty": false
                    },
                    "value": 0
                },
                "tooltip": [
                    {
                        "field": "formattedTruncDate",
                        "type": "ordinal",
                        "title": "Date"
                    },
                    {
                        "field": "formattedRawValue",
                        "type": "ordinal",
                        "title": "Value"
                    },
                    {
                        "field": "tooltipText",
                        "type": "nominal",
                        "title": "TooltipText"
                    },
                    {
                        "field": "tooltipMarkup",
                        "type": "nominal",
                        "title": "TooltipMarkup"
                    }
                ]
            },
            "params": [
                {
                    "name": "hover",
                    "select": {
                        "type": "point",
                        "fields": [
                            "truncDate"
                        ],
                        "nearest": true,
                        "on": "mouseover",
                        "clear": "mouseout"
                    }
                }
            ],
            "name": "hover-tooltips"
        },
        {
            "name": "hover-dot",
            "description": "Hover dot on main line for a single time series data point",
            "mark": {
                "opacity": 1,
                "type": "point",
                "size": {
                    "expr": "hoverDotSize"
                },
                "fill": {
                    "expr": "hoverDotColor"
                },
                "stroke": null
            },
            "encoding": {
                "opacity": {
                    "condition": {
                        "value": 1,
                        "param": "hover",
                        "empty": false
                    },
                    "value": 0
                },
                "y": {
                    "type": "quantitative",
                    "axis": null,
                    "field": "rawValue"
                }
            }
        }
    ],
    "view": {
        "stroke": null
    },
    "resolve": {
        "axis": {
            "y": "independent"
        }
    },
    "legend": {
        "explanations": [
            {
                "text": "The range that the metric value is expected to fall in, calculated based on the 17 data points since July 1, 2024.",
                "elements": [
                    "expected_range"
                ]
            }
        ],
        "elements": [
            {
                "name": "completed",
                "label": "Completed",
                "mark": {
                    "type": "line",
                    "strokeWidth": {
                        "param": "lineStrokeWidth"
                    },
                    "strokeColor": {
                        "param": "lineColor"
                    }
                }
            },
            {
                "name": "expected_range",
                "label": "Expected range",
                "mark": {
                    "type": "square",
                    "fillColor": {
                        "param": "normalRangeColor"
                    },
                    "strokeColor": {
                        "param": "lineColor"
                    },
                    "strokeWidth": {
                        "param": "normalRangeStrokeWidth"
                    }
                }
            }
        ]
    },
    "encoding": {
        "x": {
            "type": "ordinal",
            "axis": {
                "values": {
                    "expr": "xAxisLabelsDataValues"
                },
                "labelFontWeight": {
                    "condition": [
                        {
                            "test": "activeLabelAndGridDate === datum.value",
                            "expr": "xAxisLabelFontWeightActive"
                        }
                    ],
                    "expr": "xAxisLabelFontWeightDefault"
                },
                "grid": true,
                "gridColor": {
                    "expr": "xAxisGridColorDefault"
                },
                "offset": {
                    "expr": "xAxisOffset"
                },
                "labelAlign": {
                    "condition": [
                        {
                            "test": "datum.index === 0",
                            "value": "left"
                        },
                        {
                            "test": "datum.index === 1",
                            "value": "right"
                        }
                    ],
                    "value": "center"
                },
                "gridOpacity": {
                    "expr": "datum.index === 0 || datum.truncDate === activeLabelAndGridDate ? 0 : 1"
                },
                "format": {
                    "custom": true,
                    "mapName": "xAxis"
                },
                "labelOverlap": true
            },
            "scale": {
                "type": "point",
                "padding": {
                    "expr": "xAxisScalePadding"
                }
            },
            "field": "truncDate"
        }
    }
}